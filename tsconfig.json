{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "declaration": true,
      "noImplicitAny": false,
      "sourceMap": true,
      "rootDir": "./src",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true, // TODO: Maybe should remove this, but this fixes bug with current version of discord.js
      "allowJs": true
    },
    "exclude": [
      "dist",
      "node_modules"
    ]
  }
