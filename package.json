		"@discordjs/voice": "^0.17.0",
		"@discordx/importer": "^1.3.1",
		"@discordx/pagination": "^3.5.5",
		"@discordx/utilities": "^7.0.3",
		"@types/glob": "^8.1.0",
		"axios": "^1.7.7",
		"cheerio": "^1.0.0-rc.10",
		"cross-env": "^7.0.3",
		"csgo-predict-api": "^1.1.8",
		"discord-api-types": "^0.37.101",
		"discord-player": "^6.7.1",
		"discord.js": "^14.16.2",
		"discordx": "^11.12.1",
		"dotenv": "^8.6.0",
		"dynamoose": "^4.0.1",
