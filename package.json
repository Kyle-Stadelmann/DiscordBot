{
	"name": "bddiscordbot",
	"version": "3.0.1",
	"description": "BD4 Discord Bot",
	"main": "app.ts",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"start": "ts-node --esm -r dotenv/config src/app.ts",
		"start-prod": "cross-env BDBOT_PROD_MODE=TRUE node -r dotenv/config dist/app.js",
		"format": "npx prettier src/ --write",
		"lint": "npx eslint src/",
		"dev": "nodemon"
	},
	"author": "Kyle Stadelmann",
	"license": "ISC",
	"dependencies": {
		"@braintree/sanitize-url": "^7.1.0",
